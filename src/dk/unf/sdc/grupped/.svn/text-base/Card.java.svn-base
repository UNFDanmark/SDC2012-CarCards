package dk.unf.sdc.grupped;

import android.graphics.drawable.Drawable;

public class Card {

	private String brand;
	private int picture;
	private int speed;
	private double engineSize;
	private double nullToHundred;
	private int horsePower;
	private int usePerKm;

	public Card(String br, int pic, int fart, double engine, double acceleration, int power, int kml) {
		brand = br;
		picture = pic;
		speed = fart;
		engineSize = engine;
		nullToHundred = acceleration;
		horsePower = power;
		usePerKm = kml;
	}

	public String getBrand() {
		return brand;
	}

	public int getPicture() {
		return picture;
	}

	public int getSpeed() {
		return speed;
	}

	public double getEngineSize() {
		return engineSize;
	}

	public double getNullToHundred() {
		return nullToHundred;
	}

	public int getHorsePower() {
		return horsePower;
	}

	public int getUsePerKm() {
		return usePerKm;
	}

}
